Match host * exec "gpg-connect-agent UPDATESTARTUPTTY /bye"

Host diary
    HostName thesiah.xyz
    User root
    RequestTTY yes
    RemoteCommand cd /var/www/thesiah/diary && exec $SHELL -l

Host github
    HostName github.com
    User git
    IdentityAgent ~/.gnupg/S.gpg-agent.ssh
    IdentitiesOnly yes

